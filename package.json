{
  "name": "app",
  "description": "App",
  "version": "0.0.4",
  "scripts": {
    "start": "gulp run",
    "build": "gulp build",
    "bstart": "NODE_ENV=production node build/backend.js",
    "install": "grunt build",
    "deploy": "git push deis-staging master",
    "deploy-prod": "git push deis-prod master",
    "deploy-qa": "git push deis-qa loadDocs-refactor",
    "clean": "find . -name 'derby-serialized' -type d | xargs /bin/rm -rf && npm run clean-js && npm run clean-maps",
    "compile": "find . -name '*.coffee' -type f | grep -v './node_modules' | grep -v './test' | xargs $( pwd )/node_modules/.bin/coffee --compile",
    "clean-js": "find . -name '*.coffee' -type f | grep -v 'node_modules' | sed -e 's/\\.coffee/\\.js/g' | xargs /bin/rm -f"
  },
  "dependencies": {
    "async": "*",
    "coffee-script": "~1.7.1",
    "crypto": "0.0.3",
    "d-connection-alert": "~0.1.0",
    "derby": "git://github.com/dmapper/derby#webpack",
    "derby-invite": "git://github.com/dmapper/derby-invite#webpack",
    "derby-login": "0.4.13",
    "derby-parsing": "git://github.com/dmapper/derby-parsing#webpack",
    "derby-templates": "git://github.com/dmapper/derby-templates#webpack",
    "derby-webpack": "^0.2.0",
    "dm-form": "^0.4.12",
    "dm-stripe": "^0.1.0",
    "dm-util": "^1.0.1",
    "express": "~4.8.7",
    "faker": "^3.0.1",
    "fs": "0.0.2",
    "gulp": "^3.9.0",
    "install": "^0.4.1",
    "json2csv": "^2.9.0",
    "livedb": "~0.5.9",
    "local-derby-server": "1.0.3",
    "lodash": "*",
    "mailgun-js": "^0.7.7",
    "moment": "^2.10.6",
    "moment-timezone": "^0.4.0",
    "multiparty": "^4.1.2",
    "nconf": "~0.6.8",
    "npm": "^3.5.2",
    "qs": "~5.2.0",
    "racer": "0.6.0-alpha32",
    "superagent": "^1.6.1",
    "yamlify": "~0.1.0"
  },
  "engines": {
    "node": "0.10.x"
  },
  "private": true,
  "devDependencies": {
    "grunt": "*",
    "grunt-bower-task": "*",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-clean": "*",
    "grunt-contrib-coffee": "*",
    "grunt-contrib-concat": "*",
    "grunt-contrib-copy": "*",
    "grunt-contrib-uglify": "*",
    "grunt-notify": "*",
    "grunt-shell": "*",
    "grunt-svg-sprite": "^1.1.0",
    "grunt-wait-server": "*",
    "load-grunt-tasks": "*",
    "nodemon": "~1.3.1"
  }
}
