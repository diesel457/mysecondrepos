@font-face {
    font-family 'Regular'
    src url('/fonts/Regular/myriad-set-pro_text.eot')
    src url('/fonts/Regular/myriad-set-pro_text.eot?#iefix') format('embedded-opentype'),
         url('/fonts/Regular/myriad-set-pro_text.woff') format('woff'),
         url('/fonts/Regular/myriad-set-pro_text.ttf') format('truetype'),
         url('/fonts/Regular/myriad-set-pro_text.svg#myriad_set_protext') format('svg')
    font-weight normal
    font-style normal

}

@font-face {
    font-family 'Thin'
    src url('/fonts/Thin/myriad-set-pro_thin.eot')
    src url('/fonts/Thin/myriad-set-pro_thin?#iefix') format('embedded-opentype'),
         url('/fonts/Thin/myriad-set-pro_thin.woff') format('woff'),
         url('/fonts/Thin/myriad-set-pro_thin.ttf') format('truetype'),
         url('/fonts/Thin/myriad-set-pro_thin.svg#myriad_set_protext') format('svg')
    font-weight normal
    font-style normal

}
